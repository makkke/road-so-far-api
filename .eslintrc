{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "mocha": true,
  },
  "ecmaFeatures": {
    "jsx": true,
    "es6": true,
    "classes": true,
  },
  "rules": {
    "semi": [2, "never"],
    "comma-dangle": [1, "always-multiline"],
    "max-len": [1, 110, 4],
    "arrow-body-style": [0],
    "consistent-return": [0],
    "no-underscore-dangle": [0],
    "no-unused-expressions": [0],
  },
}